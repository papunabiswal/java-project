pipeline {
    agent any
    tools {
        maven "project-maven"
    }
    // environment {
    //     scannerHome = tool "SonarScanner"
    // }
    stages {
        stage('Git Checkout') {
            steps {
                git credentialsId: 'GitHub', url: 'https://github.com/papunabiswal/java-project.git'
            }
        }
        stage('Maven Goal') {
            steps {
            script{
                sh "mvn clean install package"
            }
            }
        } 
    }
}